pipeline {
   agent any
    stages {
         stage('pre -build') {
            steps {
                sh 'echo Pre-build'
                sh 'python3 --version'
            }
        }
        stage('build') {
            steps {
                
                echo 'build in progress'
                sh 'python3  Webhook/main.py'
            }
        }
        stage('test1') {
            steps {
                
                echo 'test1 in progress'
            }
        }
        stage('test2') {
            steps {
                
                echo 'test2 in progress'
            }
        }
    }
    post {
        always {
            echo 'Cleanup after everything!'
        }
        success {
            echo 'success 123'
        }
    }
}
